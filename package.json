{
  "name": "vlm-ocr-interface",
  "version": "1.0.0",
  "description": "A simple VLM OCR interface...",
  "scripts": {
    "build:scss": "sass src/scss/main.scss:dist/output.css --style compressed",
    "watch:scss": "sass src/scss/main.scss:dist/output.css --watch",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview": "vite preview",
    "build": "npm run build:scss && npm run build:vite",
    "dev": "npm-run-all --parallel watch:scss dev:vite"
  },
  "keywords": [
    "OCR",
    "VLM",
    "SCSS",
    "TypeScript",
    "HTML"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.87.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
